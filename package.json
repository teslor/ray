{
  "name": "ray",
  "version": "0.2.0",
  "author": {
    "name": "Pavel D.",
    "email": "pvd367@gmail.com"
  },
  "description": "Desktop HTML-powered text editor with advanced features",
  "keywords": ["html", "text", "editor", "viewer", "note", "wysiwyg"],
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix",
    "prepare": "patch-package"
  },
  "build": {
    "productName": "Ray",
    "appId": "com.electron.ray",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "mac": {
      "icon": "build/icons/icons.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    },
    "nsis": {
      "artifactName": "${productName}-${version}-x64.${ext}"
    }
  },
  "dependencies": {
    "electron-store": "^1.3.0",
    "element-ui": "^2.0.11",
    "file-uri-to-path": "^1.0.0",
    "findandreplacedomtext": "^0.4.6",
    "jquery": "^3.3.1",
    "jquery-resizable-dom": "^0.26.0",
    "jstree": "^3.3.5",
    "lodash": "^4.17.4",
    "mousetrap": "^1.6.1",
    "quill": "^1.3.5",
    "quill-magic-url": "0.0.5",
    "vue": "^2.5.13",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.8.2-beta.5",
    "electron-builder": "^19.55.3",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.3",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "patch-package": "^5.0.0",
    "postcss": "^6.0.16",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.10",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.7.0",
    "vue-style-loader": "^3.1.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0"
  }
}
